<mvc:View controllerName="dtt.SAPUI5.controller.App" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true">
    <Shell id="_IDGenShell1">
        <App id="_IDGenApp1">
            <pages>
                <Page id="_IDGenPage1" title="{i18n>appTitle}">
                    <headerContent>
                        <Button id="HelloDialogHeader" icon="sap-icon://action" press=".onOpenDialogHeader" class="sap.ui.smallMarginEnd" />
                    </headerContent>
                    <content>
                        <mvc:XMLView id="_IDGenXMLView1" viewName="dtt.SAPUI5.view.HelloPanel" />
                        <mvc:XMLView id="_IDGenXMLView2" viewName="dtt.SAPUI5.view.InvoicesList" />
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
